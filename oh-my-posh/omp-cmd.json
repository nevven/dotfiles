{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "<#F3AE35,transparent>\ue0b6</><,#F3AE35> > </><#F3AE35,transparent>\ue0b0</> ",
    "foreground": "#000000",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u256d\u2500",
          "type": "session",
          "style": "plain"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} ",
          "foreground": "#000000",
          "background": "#F07623",
          "type": "session",
          "style": "diamond"
        },
        {
          "options": {
            "style": "full"
          },
          "template": " {{ .Folder }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#F3AE35",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf7a3"
          },
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#59C9A5",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#000000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ffffff{{ end }}",
            "{{ if gt .Ahead 0 }}#ffffff{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#F3AE35{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#D81E5B{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#F3AE35",
          "type": "root",
          "style": "powerline"
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": " {{ if gt .Code 0 }}\uf135{{ else }}\uf135{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#2f91af",
          "type": "status",
          "style": "powerline",
          "background_templates": [
            "{{ if gt .Code 0 }}#D81E5B{{ end }}"
          ]
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#F07623",
          "type": "python",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_package_manager": false,
            "home_enabled": false
          },
          "template": "\uf898 ",
          "foreground": "#59C9A5",
          "background": "transparent",
          "type": "node",
          "style": "plain"
        },
        {
          "options": {
            "time_format": "15:04:05 Monday"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#2f91af",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "type": "text",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
