{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    
    "transient_prompt": {
        "foreground": "#809399",
        "background": "transparent",
        "template": "‚ùØ "
    },
    "secondary_prompt": {
        "style": "plain",
        "foreground": "#809399",
        "background": "transparent",
        "template": " ‚ùØ "
    },
    "console_title_template": "{{ .Folder }}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "template": "\u256d\u2500",
                    "type": "session",
                    "style": "plain",
                    "foreground": "#809399"
                },
                {
                    "style": "plain",
                    "foreground": "#c95ce4",
                    "background": "transparent",
                    "template": " ~ {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} ",
                    "type": "session"
                },
                {
                    "options": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "foreground": "#c78126",
                    "background": "transparent",
                    "template": "<#d3d3d3>‚ùØ</> {{ .Path }} ",
                    "type": "path"
                },
                {
                    "options": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": "\uf7a3"
                    },
                    "style": "plain",
                    "foreground": "#2fd8a2",
                    "background": "transparent",
                    "template": "<#d3d3d3>‚ùØ</> {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
                    "type": "git",
                    "foreground_templates": [
                        "{{ if .Working.Changed }}#fd6e86{{ end }}",
                        "{{ if .Staging.Changed }}#1ead5a{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#D81E5B{{ end }}",
                        "{{ if gt .Ahead 0 }}#3895ff{{ end }}",
                        "{{ if gt .Behind 0 }}#7A306C{{ end }}"
                    ]
                },
                {
                    "options": {
                        "cache_duration": "none"
                    },
                    "style": "plain",
                    "foreground": "#809399",
                    "background": "transparent",
                    "template": "<#d3d3d3>‚ùØ</> üó≤ ",
                    "type": "root"
                },
                {
                    "options": {
                        "cache_duration": "none",
                        "display_mode": "context",
                        "fetch_virtual_env": true
                    },
                    "style": "plain",
                    "foreground": "#c95ce4",
                    "background": "transparent",
                    "template": "<#d3d3d3>‚ùØ</>  \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
                    "type": "python"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "options": {
                        "time_format": "Monday, 02 15:04"
                    },
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "foreground": "#309bbb",
                    "type": "time",
                    "style": "plain"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "options": {
                        "cache_duration": "none"
                    },
                    "template": "\u2570\u2500",
                    "type": "text",
                    "style": "plain",
                    "foreground": "#809399"
                },
                {
                    "options": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "foreground": "#809399",
                    "background": "transparent",
                    "template": "‚ùØ‚ùØ",
                    "type": "status",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#D81E5B{{ end }}"
                    ]
                }
            ],
            "newline": true
        }
    ],
    "version": 3,
    "final_space": true
}